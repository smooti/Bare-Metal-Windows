			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
			<UserAccounts>
				<AdministratorPassword>
					<Value>MQBxAGEAegAyAHcAcwB4ACEAUQBBAFoAQABXAFMAWABBAGQAbQBpAG4AaQBzAHQAcgBhAHQAbwByAFAAYQBzAHMAdwBvAHIAZAA=</Value>
